<div id="{{ id }}" class="mb-element mb-element-dimensionshandler" title="{{ title |trans }}">
{% for key,dimensionset in configuration.dimensionsets %}
  <div id="{{key}}" class="dimensionset row no-gutters">
    <div class="col-xs-4 col-lg-3 dimensionset-title">{{ dimensionset.title ?: (dimensionset.dimension.name | humanize) }}</div>
    <div class="col-xs-4 col-lg-6 slider-wrap"><div class="mb-slider" id="dimensionset-{{key}}"></div></div>
    <div class="col-xs-4 col-lg-3 dimensionset-value"></div>
  </div>
{% endfor %}
</div>
